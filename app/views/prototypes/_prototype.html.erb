<% @prototypes.each do |prototype| %>
<div class="card" >
  <div class="card__img">
  <%= link_to image_tag(prototype.image.variant(resize: '200x150')), prototype_path(prototype) %>
  </div>
  <p class="card__title"><%= prototype.title %></p>
  <p class="summary"><%= prototype.catch_copy %></p>
  <p class="card__user">by<%= prototype.user.name %></p>
  </div>
<% end %>
